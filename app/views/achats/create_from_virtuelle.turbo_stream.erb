<%= turbo_stream.replace "achats" do %>
	<%= render "achats/achats", achats: @achats %>
<% end %>

<%= turbo_stream.append "achats_chantier_#{@chantier.id}" do %>
	<%= render "achats/achat", achat: @achat %>
<% end %>

<% @utilisations.each do |utilisation| %>
	<%= turbo_stream.replace "utilisation_#{utilisation.id}" do %>
		<%= render "utilisations/utilisation", utilisation: utilisation %>
	<% end %>
<% end %>

<%= turbo_stream.remove @dom_id do %>
<% end %>

<%= turbo_stream.replace "chantier_ressources" do %>
	<%= render "chantiers/ressources", chantier: @chantier %>
<% end %>

<%= turbo_stream.replace "commande_ressources" do %>
	<%= render "commandes/ressources", commande: @commande %>
<% end %>
